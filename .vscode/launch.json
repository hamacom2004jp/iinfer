    "version": "0.0.1",
    "configurations": [
        {
            "name": "Python: iinfer server",
            "type": "python",
            "request": "launch",
            "args": ["-m","server","-c","start","-f","--retry_count","3","--debug"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer client",
            "type": "python",
            "request": "launch",
            "args": ["-m","postprocess","-c","httpreq","--input_file","pred.json","--json_connectstr","http://localhost:8081/showimg/pub_img"],
            //"args": ["--version"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer gui",
            "type": "python",
            "request": "launch",
            "args": ["-m","gui","-c","start","-f"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer web",
            "type": "python",
            "request": "launch",
            "args": ["-m","web","-c","start", "--signin_file",".samples/user_list.yml","-f"],//,"--debug","--client_only", "--filer_html", "filer2.html", "--assets", "README.md", "--assets", "setup.py"
            //"args": ["--version"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer webcap",
            "type": "python",
            "request": "launch",
            "args": ["-m","web","-c","webcap","--listen_port", "8082",
                     "--image_type", "capture", "--capture_count", "1", "--capture_frame_width", "1920", "--capture_frame_height", "1080", "--capture_fps", "5"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer read_dir",
            "type": "python",
            "request": "launch",
            "args": ["-m","client","-c","read_dir","-f", "--glob_str", "**/*.jpg", "--read_input_type", "jpeg", "--image_type", "capture",
                     "--root_dir", "sample/input", "--polling_interval", "1", "--polling"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: iinfer install",
            "type": "python",
            "request": "launch",
            "args": ["-m","install","-c","mmdet","-f"],
            "module": "iinfer",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false
        },
        {
            "name": "Python: gentest",
            "type": "debugpy",
            "request": "launch",
            "program": "iinfer/tools/gentest.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": false,
            "env": {
                "PYTHONPATH":"${workspaceFolder}", // 追加
            }
        }
        
    ]
}